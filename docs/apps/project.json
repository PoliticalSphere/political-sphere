{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/docs",
  "projectType": "application",
  "tags": ["docs"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/docs && npm run build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/docs && npm run serve"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run docs:lint"
      }
    }
  }
}
