{
  "executionMode": "Safe",
  "implicitContexts": {
    "repo_read": true,
    "tests_run": true,
    "terminal_run": true,
    "git_read": true,
    "pr_create": "draft_only",
    "changelog_todo_edit": true,
    "ephemeral_cache": true,
    "audit_logging": true
  },
  "explicitApprovalRequired": [
    "repo_write",
    "secrets_access",
    "external_network",
    "package_publish",
    "infra_deploy"
  ],
  "maxImplicitChangeSize": 300,
  "logRetentionDays": 90,
  "defaultPrMode": "Safe"
}
